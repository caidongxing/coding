package com.renjun.po;

import java.math.BigDecimal;

public class yw_CollectFeesInformation {
    private Integer collectfeesinformationid;

    private Integer chargingitemid;

    private BigDecimal chargeamount;

    private String remarks;

    private String chargingitemname;
    
    public Integer getCollectfeesinformationid() {
        return collectfeesinformationid;
    }

    public void setCollectfeesinformationid(Integer collectfeesinformationid) {
        this.collectfeesinformationid = collectfeesinformationid;
    }

    public Integer getChargingitemid() {
        return chargingitemid;
    }

    public void setChargingitemid(Integer chargingitemid) {
        this.chargingitemid = chargingitemid;
    }

    public BigDecimal getChargeamount() {
        return chargeamount;
    }

    public void setChargeamount(BigDecimal chargeamount) {
        this.chargeamount = chargeamount;
    }

    public String getRemarks() {
        return remarks;
    }

    public void setRemarks(String remarks) {
        this.remarks = remarks == null ? null : remarks.trim();
    }

	public String getChargingitemname() {
		return chargingitemname;
	}

	public void setChargingitemname(String chargingitemname) {
		this.chargingitemname = chargingitemname;
	}
    
    
}